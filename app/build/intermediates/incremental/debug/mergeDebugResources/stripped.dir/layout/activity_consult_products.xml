<?xml version="1.0" encoding="utf-8"?>

                                                       
                                                   

    

           

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingTop="16dp" android:tag="layout/activity_consult_products_0" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Consult_Products"
            android:layout_gravity="center"
            android:textSize="30dp"
            />

        <Spinner
            android:id="@+id/spinnerProducts"
            android:layout_marginTop="20dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/eq_recycler"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            tools:itemCount="2"
            tools:listitem="@layout/consultmenus_item"/>

        <Button
            android:id="@+id/ButtonReturn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Return"
            android:layout_marginTop="20dp"
            />
    </LinearLayout>
         